{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "importHelpers": true,
    "module": "ES2022",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "experimentalDecorators": true,
    "target": "ES2022",
    "allowSyntheticDefaultImports": true,
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2017",
      "dom"
    ],
    "paths": {
      "firebaseui-angular-v2": [
        "dist/firebaseui-angular-library"
      ],
      "firebaseui-angular-v2/*": [
        "dist/firebaseui-angular-library/*"
      ]
    },
    "useDefineForClassFields": false
  }
}
